{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions",
            "not ie <= 8"
          ]
        }
      }
    ]
  ],
  "plugins": [
    "transform-vue-jsx",
    "transform-runtime",
    "transform-object-rest-spread",
    "syntax-dynamic-import",
    "lodash",
    [
      "component"
    ]
  ],
  "env": {
    "coverage": {
      "presets": ["env", "stage-2"]
    }
  }
}
